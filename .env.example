# .env.example

# 安全配置
SECRET_KEY=your_super_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=60
ALGORITHM=HS256

# 数据库（开发阶段可用 SQLite）
DATABASE_URL=sqlite:///./lyt2.db

# 模型接口（可替换为豆包等）
OPENAI_API_KEY=your_openai_key
MODEL_PROVIDER=openai  # 或 doubao / local

# 前端访问端口
FRONTEND_PORT=5173

# 后端API服务
API_HOST=0.0.0.0
API_PORT=8000

